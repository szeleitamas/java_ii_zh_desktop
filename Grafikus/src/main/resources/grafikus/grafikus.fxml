    <Label fx:id="lb1">Adatok</Label>
    <GridPane fx:id="gp1" hgap="2" vgap="4">
        <Label GridPane.rowIndex="0" GridPane.columnIndex="0">Nemzeti Park Név:</Label>
        <TextField fx:id="tfnev" GridPane.rowIndex="0" GridPane.columnIndex="1"></TextField>
        <Label GridPane.rowIndex="1" GridPane.columnIndex="0">Igazgató:</Label>
        <TextField fx:id="tfigazgato" GridPane.rowIndex="1" GridPane.columnIndex="1"></TextField>
        <Label GridPane.rowIndex="2" GridPane.columnIndex="0">Kinevezés:</Label>
        <TextField fx:id="tfkinevezes" GridPane.rowIndex="2" GridPane.columnIndex="1"></TextField>
        <Button onAction="#bt1Click" GridPane.rowIndex="3" GridPane.columnIndex="1" >Küldés</Button>
    </GridPane>
    <TableView fx:id="tv1" >
    </TableView>

    <Label fx:id="lb2">Update.(3-as id adatait változtatja meg mindig.</Label>
