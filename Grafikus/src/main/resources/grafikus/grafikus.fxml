            <TextField fx:id="tf6" promptText="User ID" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button mnemonicParsing="false" onAction="#btnRest1ReadClick" text="User lekérdezés" GridPane.columnIndex="1" GridPane.rowIndex="5" />
