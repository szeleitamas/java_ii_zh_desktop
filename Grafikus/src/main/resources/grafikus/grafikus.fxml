                </items>
                <items>
                    <MenuItem text="Töröl" onAction="#menuDeleteClick"/>
                </items>
            </Menu>
            <Menu text="Rest1">
                <items>
                    <MenuItem text="Create" onAction="#Rest1CreateClick"/>
                </items>
                <items>
                    <MenuItem text="Read" onAction="#Rest1ReadClick"/>
                </items>
                <items>
